<%= link_to 'New Image', new_image_path %>
<ul id="images_list">
  <% @images.each do |image| %>
    <li class="image_item">
      <%= link_to (image_tag image.url, class: 'index-img'), image%>
      <% image.tag_list.each do |tag| %>
        <%= link_to(tag, images_path(tag: tag), class: 'tag') %>
      <% end %>
    </li>
  <% end %>
</ul>
